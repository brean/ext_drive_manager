SelectAction {
    align: center middle;
}

#dialog > ListItem > Label {
    padding: 1 2;
}

#dialog {
    grid-size: 2;
    grid-gutter: 1 2;
    grid-rows: 1fr 3;
    padding: 0 1;
    width: 60;
    height: 11;
    border: $primary;
    background: $surface;
}

.warning.-highlight {
    background: $warning;
}